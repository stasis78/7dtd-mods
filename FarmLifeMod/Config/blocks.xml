      <!-- Master Meat Blocks -->
      <block name="emptyAnimalPenMaster">
        <property name="CreativeMode" value="None"/>
        <property name="DisplayType" value="blockMulti" />
      	<property name="MultiBlockDim" value="3,1,3" />
        <property name="Material" value="Miron_scrap"/>
      	<property name="MaxDamage" value="2000"/>
        <property name="Shape" value="ModelEntity"/>
      	<property name="Path" value="solid"/>
      	<property name="Model" value="Entities/Industrial/pallet_stackPrefab"/>
        <property name="Collide" value="movement,melee,bullet,arrow,rocket" />
        <property name="FilterTags" value="fdecor,fother,ffurniture" />
        <drop event="Destroy" count="0"/>
      </block>

      <block name="growingAnimalPenMaster">
        <property name="Extends" value="emptyAnimalPenMaster" param1=""/>
        <property name="Shape" value="ModelEntity"/>
      	<property name="Path" value="solid"/>
      	<property name="Model" value="Entities/Industrial/pallet_tarp_tilePrefab"/>
        <property name="Class" value="UpgradeRated" />
        <property name="UpgradeRated.ToBlock" value="fullAnimalPenMaster" />
        <property name="UpgradeRated.Rate" value="2" />
      </block>

      <block name="fullAnimalPenMaster">
        <property name="Extends" value="emptyAnimalPenMaster" param1=""/>
        <property name="Shape" value="ModelEntity"/>
        <property name="Path" value="solid"/>
        <property name="Model" value="Entities/Industrial/pallet_boxes_basePrefab"/>
        <property name="Material" value="Morganic"/>
